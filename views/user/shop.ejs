<head>
    <meta charset="utf-8">
    <title>OZORA</title>
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta property="og:title" content="">
    <meta property="og:type" content="">
    <meta property="og:url" content="">
    <meta property="og:image" content="">
    <!-- Favicon -->
    <link rel="shortcut icon" type="image/x-icon" href="assets/imgs/theme/favicon.svg">
    <!-- Template CSS -->
    <link rel="stylesheet" href="assets/css/maind134.css?v=3.4">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css">
    <style>
        .star-rating {
            display: flex;
        }

        .star {
            cursor: pointer;
            margin-right: 5px;
        }

        input[type="checkbox"] {
            display: none;
        }

        input[type="checkbox"]:checked+label {
            color: gold;
        }
    </style>
</head>

<body>

    <%- include('header') %>

    <main class="main">
        <div class="page-header breadcrumb-wrap">
            <div class="container">
                <div class="breadcrumb">
                    <a href="index.html" rel="nofollow">Home</a>
                    <span></span> Shop
                </div>
            </div>
        </div>
        <section class="mt-50 mb-50">
            <div class="container">
                <div class="row flex-row-reverse">
                    <div class="col-lg-9">
                        <div class="shop-product-fillter">
                            <div class="totall-product">
                                <p> We found <strong class="text-brand"><%=products.length%></strong> items for you!</p>
                            </div>
                            <div class="sort-by-product-area">
                                
                                <div class="sort-by-cover">
                                    <div class="sort-by-product-wrap">
                                        <div class="sort-by">
                                            <span><i class="fi-rs-apps-sort"></i>Sort by:</span>
                                        </div>
                                        <div class="sort-by-dropdown-wrap">
                                            <span> Price <i class="fi-rs-angle-small-down"></i></span>
                                        </div>
                                    </div>
                                    <div class="sort-by-dropdown">
                                        <ul>
                                            <% if (locals.categoryId) { %>
                                                <li><a href="/shop?categoryId=<%= categoryId %>&message=<%= message %>&sort=ascending&page=<%= currentPage %>">Price: Low to High</a></li>
                                                <li><a href="/shop?categoryId=<%= categoryId %>&message=<%= message %>&sort=descending&page=<%= currentPage %>">Price: High to Low</a></li>
                                            <% } else if (locals.brandId) { %>
                                                <li><a href="/shop?brandId=<%= brandId %>&message=<%= message %>&sort=ascending&page=<%= currentPage %>">Price: Low to High</a></li>
                                                <li><a href="/shop?brandId=<%= brandId %>&message=<%= message %>&sort=descending&page=<%= currentPage %>">Price: High to Low</a></li>
                                            <% } else if (locals.gender) { %>
                                                <li><a href="/shop?gender=<%= gender %>&message=<%= message %>&sort=ascending&page=<%= currentPage %>">Price: Low to High</a></li>
                                                <li><a href="/shop?gender=<%= gender %>&message=<%= message %>&sort=descending&page=<%= currentPage %>">Price: High to Low</a></li>
                                            <% } else { %>
                                                <li><a href="/shop?message=<%= message %>&sort=ascending&page=<%= currentPage %>">Price: Low to High</a></li>
                                                <li><a href="/shop?message=<%= message %>&sort=descending&page=<%= currentPage %>">Price: High to Low</a></li>
                                            <% } %>
                                        </ul>
                                    </div>
                                    
                                   
                                </div>
                            </div>
                        </div>
                        <div class="row product-grid-3">
                                <%for(i=0;i<products.length;i++){%>
                                    <div class="col-lg-4 col-md-4 col-12 col-sm-6                                                          ">
                                        <div class="product-cart-wrap mb-30">
                                            <div class="product-img-action-wrap">
                                                <div class="product-img product-img-zoom">
                                                    <a href="/productDetails?id=<%=products[i]._id%>">
                                                        <img class="default-img" src="/<%=products[i].image[0]%>" alt="">
                                                        <img class="hover-img" src="/<%=products[i].image[0]%>" alt="">
                                                    </a>
                                                </div>
                                                <div class="product-action-1">
                                                    <!-- <a aria-label="Quick view" class="action-btn hover-up" data-bs-toggle="modal" data-bs-target="#quickViewModal">
                                                    <i class="fi-rs-search"></i></a> -->
                                                    <a aria-label="Add To Wishlist" class="action-btn hover-up" href="shop-wishlist.html"><i class="fi-rs-heart"></i></a>
                                                    <!-- <a aria-label="Compare" class="action-btn hover-up" href="shop-compare.html"><i class="fi-rs-shuffle"></i></a> -->
                                                </div>
                                                <div class="product-badges product-badges-position product-badges-mrg">
                                                    <%if(products[i].tags.length>0){%>
                                                        <span class="hot"><%=products[i].tags%></span>
                                                        <%}else{%>
                                                            <span></span>
                                                            <%}%>
                                                </div>
                                            </div>
                                            <div class="product-content-wrap">
                                                <div class="product-category">
                                                    <a href="/productDetails?id=<%=products[i]._id%>"><%=products[i].brandId.brandName%></a>
                                                </div>
                                                <h2><a href="/productDetails?id=<%=products[i]._id%>"><%=products[i].title%></a></h2>
                                                <%if(products[i].rating!=null){%>
                                                    <div class="star-rating" id="ratingStars">
                                                        <% for (let j = 1; j <= 5; j++) { %>
                                                            <input disabled type="checkbox" id="star<%= j %>" <% if (Math.round(products[i].rating/100*5) >= j) { %>checked<% } %> name="rating" value="<%= j %>"/>
                                                            <label for="star<%= j %>" class="star"><i class="fas fa-star"></i></label>
                                                        <% } %>
                                                    </div>
                                                    <%}%>
                                                <div class="product-price">
                                                    <span>र<%=products[i].salePrice.toFixed(2)%></span>
                                                    <span class="old-price">र<%=products[i].regularPrice.toFixed(2)%></span>
                                                </div>
                                                <!-- <div class="product-action-1 show">
                                                    <a aria-label="Add To Cart" class="action-btn hover-up" href="shop-cart.html"><i class="fi-rs-shopping-bag-add"></i></a>
                                                </div> -->
                                            </div>
                                        </div>
                                    </div>
        
                                    <%}%>
                            
                        </div>
                        <!--pagination-->
                        <div class="pagination-area mt-15 mb-sm-5 mb-lg-0">
                            <nav aria-label="Page navigation example">
                                <ul class="pagination justify-content-start">
                                    <% for (let i = 1; i <= totalPages; i++) { %>
                                        <li class="page-item <%= currentPage === i ? 'active' : '' %>">
                                            <a class="page-link" href="?page=<%= i %>&brandId=<%= brandId %>&categoryId=<%= categoryId %>&message=<%= message %>&gender=<%= gender %>"><%= i %></a>
                                        </li>
                                    <% } %>
                                </ul>
                            </nav>
                        </div>
                    </div>
                    <div class="col-lg-3 primary-sidebar sticky-sidebar">
                        <div class="widget-category mb-30">
                            <h5 class="section-title style-1 mb-30 wow fadeIn animated">Category</h5>
                            <ul class="categories">
                                <%for(i=0;i<categories.length;i++){%>
                                    <li><a href="/shop?categoryId=<%=categories[i]._id%>"><%=categories[i].categoryName%></a></li>
                                    <%}%>
                            </ul>
                        </div>
                        <div class="widget-category mb-30">
                            <h5 class="section-title style-1 mb-30 wow fadeIn animated">Gender</h5>
                            <ul class="categories">
                                
                                <li><a href="/shop?gender=Mens&message=withGender">MENS</a></li>
                                <li><a href="/shop?gender=Womens&message=withGender">WOMENS</a></li>
                                <li><a href="/shop?gender=Unisex&message=withGender">UNISEX</a></li>
                               
                                
                            </ul>
                        </div>
                       
                        <div class="widget-category mb-30"> 
                            <h5 class="section-title style-1 mb-30 wow fadeIn animated">Price Range</h5>
                            <ul class="categories">
                                <li><a href="/homeDeepSort?start=0&end=3000">Below 3000</a></li>
                                <li><a href="/homeDeepSort?start=3000&end=7000">3000-7000</a></li>
                                <li><a href="/homeDeepSort?start=7000&end=10000">7000-10000</a></li>
                                <li><a href="/homeDeepSort?start=10000&end=50000">Above 10000</a></li>
                                
                            </ul>
                        </div>
                       
                    </div>
                </div>
            </div>
        </section>
    </main>
    <%- include('footer') %>
    <!-- Preloader Start -->
    <div id="preloader-active">
        <div class="preloader d-flex align-items-center justify-content-center">
            <div class="preloader-inner position-relative">
                <div class="text-center">
                    <h5 class="mb-5">Now Loading</h5>
                    <div class="loader">
                        <div class="bar bar1"></div>
                        <div class="bar bar2"></div>
                        <div class="bar bar3"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Vendor JS-->
    <script src="assets/js/vendor/modernizr-3.6.0.min.js"></script>
    <script src="assets/js/vendor/jquery-3.6.0.min.js"></script>
    <script src="assets/js/vendor/jquery-migrate-3.3.0.min.js"></script>
    <script src="assets/js/vendor/bootstrap.bundle.min.js"></script>
    <script src="assets/js/plugins/slick.js"></script>
    <script src="assets/js/plugins/jquery.syotimer.min.js"></script>
    <script src="assets/js/plugins/wow.js"></script>
    <script src="assets/js/plugins/jquery-ui.js"></script>
    <script src="assets/js/plugins/perfect-scrollbar.js"></script>
    <script src="assets/js/plugins/magnific-popup.js"></script>
    <script src="assets/js/plugins/select2.min.js"></script>
    <script src="assets/js/plugins/waypoints.js"></script>
    <script src="assets/js/plugins/counterup.js"></script>
    <script src="assets/js/plugins/jquery.countdown.min.js"></script>
    <script src="assets/js/plugins/images-loaded.js"></script>
    <script src="assets/js/plugins/isotope.js"></script>
    <script src="assets/js/plugins/scrollup.js"></script>
    <script src="assets/js/plugins/jquery.vticker-min.js"></script>
    <script src="assets/js/plugins/jquery.theia.sticky.js"></script>
    <script src="assets/js/plugins/jquery.elevatezoom.js"></script>
    <!-- Template  JS -->
    <script src="assets/js/maind134.js?v=3.4"></script>
    <script src="assets/js/shopd134.js?v=3.4"></script>
</body>


