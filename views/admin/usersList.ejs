                    </a>
                </li>
            </ul>
            <hr>
            <br>
            <br>
        </nav>
    </aside>
    <main class="main-wrap">
        <%- include('header') %>
        <section class="content-main">
            <div class="content-header">
                <h2 class="content-title">Sellers list</h2>
                <div>
                    <a href="#" class="btn btn-primary"><i class="material-icons md-plus"></i> Create new</a>
                </div>
            </div>
            <div class="card mb-4">
                
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-hover" id="example">
                            <thead>
                                <tr>
                                    <th>Seller</th>
                                    <th>Email</th>
                                    <th>Mobile</th>
                                    <th>Status</th>
                                    <th class="text-end"> Action </th>
                                </tr>
                            </thead>
                            <tbody>
                                <%for(i=0;i<users.length;i++){%>

                                <tr>
                                    <td width="40%">
                                        <a href="#" class="itemside">
                                            <div class="left">
                                                <img src="/assetsb/imgs/people/avatar1.jpg" class="img-sm img-avatar" alt="Userpic">
                                            </div>
                                            <div class="info pl-3">
                                                <h6 class="mb-0 title"><%=users[i].name%></h6>
                                                
                                            </div>
                                        </a>
                                    </td>
                                    <td><%=users[i].email%></td>
                                    <td><%=users[i].mobile%></td>
                                    <%if(users[i].is_active){%>
                                        <td><span class="badge rounded-pill alert-success">Active</span></td>
                                    <%}else{%>
                                    <td><span class="badge rounded-pill alert-danger">Blocked</span></td>
                                    <%}%>
                                    <td class="text-end">
                                        <%if(users[i].is_active){%>
                                            <a  onclick="sweet('<%= users[i]._id %>')" class="btn btn-sm btn-brand rounded font-sm mt-15 bg-danger">block</a>
                                        <%}else{%>
                                            <a  onclick="sweetAlert('<%= users[i]._id %>')" class="btn btn-sm btn-brand rounded font-sm mt-15 bg-success">unblock</a>
                                        <%}%>
                                        
                                        
                                    </td>
                                </tr>
                    
                                <%}%>

                            </tbody>
                        </table>
                        <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
                        <script>
                     function sweet(userId) {
                        Swal.fire({
                            title: "Are you sure?",
                            text: "You want to block User!",
                            icon: "warning",
                            showCancelButton: true,
                            confirmButtonColor: "#3085d6",
                            cancelButtonColor: "#d33",
                            confirmButtonText: "Yes, Block User!"
                        }).then((result) => {
                            if (result.isConfirmed) {
                                Swal.fire({
                                    title: "Blocked!",
                                    text: "User has been blocked.",
                                    icon: "success"
                                }).then((res) => {
                                    if (res.isConfirmed) {
                                        // Redirect after the second confirmation
                                        window.location.href = `blockUser?id=${userId}`;
                                    }
                                });
                            }
                        });
                    }
                            
                        </script>
                        <script>
                     function sweetAlert(userId) {
                        Swal.fire({
                            title: "Are you sure?",
                            text: "You want to block User!",
                            icon: "warning",
                            showCancelButton: true,
                            confirmButtonColor: "#3085d6",
                            cancelButtonColor: "#d33",
                            confirmButtonText: "Yes, Block User!"
                        }).then((result) => {
                            if (result.isConfirmed) {
                                Swal.fire({
                                    title: "Blocked!",
                                    text: "User has been Unblocked.",
                                    icon: "success"
                                }).then((res) => {
                                    if (res.isConfirmed) {
                                        // Redirect after the second confirmation
                                        window.location.href = `unBlockUser?id=${userId}`;
                                    }
                                });
                            }
                        });
                    }
                            
                        </script>

                         <!-- table-responsive.// -->
                    </div>
                </div> <!-- card-body end// -->
            </div> <!-- card end// -->
        </section> <!-- content-main end// -->
        <%- include('footer') %>
    </main>
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
<script src="https://cdn.datatables.net/1.13.6/js/jquery.dataTables.min.js"></script>
<script src="https://cdn.datatables.net/1.13.6/js/dataTables.bootstrap5.min.js"></script>
<script>
    $(document).ready(function () {
        new DataTable('#example');

})
</script>
