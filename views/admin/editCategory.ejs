            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Categories </h2>
                    <p> Edit  a category</p>
                </div>
            </div>
            <div class="card">
                <div class="card-body">
                    <div class="row">
                        <div class="col-md-6">
                            <form action="/admin/editCategory?id=<%=category._id%>" method="post">
                                <div class="mb-4">
                                    <label for="product_name" class="form-label">Name</label>
                                    <input type="text" placeholder="Type here" class="form-control" id="product_name" name="categoryName" value="<%=category.categoryName%>"/>
                                </div>
                               
                                <div class="d-grid">
                                    <input type="submit" class="btn btn-primary" value="Update category"></input>
                                </div>
                            </form>
                        </div>
